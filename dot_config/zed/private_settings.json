{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4.5"
    },
    "always_allow_tool_actions": true,
    "model_parameters": []
  },
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "buffer_font_family": ".ZedMono",
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "One Dark"
  },
  "vim_mode": true,
  "relative_line_numbers": true,
  "cursor_blink": false
}
