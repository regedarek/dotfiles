call plug#begin('~/.vim/plugged')
Plug 'junegunn/fzf', { 'dir': '~/.fzf', 'do': './install --all' }
Plug 'benekastah/neomake'
autocmd! BufWritePost * Neomake
call plug#end()

set backspace=2
set shiftwidth=2
set laststatus=2
set expandtab
set tabstop=2
set hlsearch
set clipboard+=unnamedplus
syntax on
filetype indent on
set autoindent
set number
set nobackup
set complete=.,w,b,u
