set nocompatible
set backspace=indent,eol,start
let g:ruby_path = system('echo $HOME/.rbenv/shims')
syntax on
filetype plugin indent on
set number
set hidden
colorscheme blue
set clipboard=unnamed
set nobackup
set noswapfile
set bs=2 fo=cqrt ls=2 shm=at tw=72 ww=<,>,h,l 
set tabstop=2 shiftwidth=2 expandtab
